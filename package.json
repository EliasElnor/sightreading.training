{
  "name": "pianomode-sightreading",
  "version": "1.0.0",
  "description": "PianoMode Sight Reading Training - Professional music reading application",
  "main": "blocksy-child/assets/Sightreading-game/sightreading-engine.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint blocksy-child/assets/Sightreading-game/**/*.js",
    "lint:fix": "eslint blocksy-child/assets/Sightreading-game/**/*.js --fix",
    "format": "prettier --write \"blocksy-child/assets/Sightreading-game/**/*.{js,css}\"",
    "check": "npm run lint && npm run test",
    "prepare": "husky install"
  },
  "keywords": [
    "piano",
    "music",
    "sight-reading",
    "education",
    "wordpress"
  ],
  "author": "PianoMode Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "tone": "^14.8.49",
    "jquery": "^3.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@playwright/test": "^1.40.1",
    "@types/jest": "^29.5.11",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.9.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "blocksy-child/assets/Sightreading-game/**/*.js",
      "!blocksy-child/assets/Sightreading-game/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/website-learn.git"
  },
  "bugs": {
    "url": "https://github.com/username/website-learn/issues"
  },
  "homepage": "https://pianomode.com"
}
